#include <fstream>
using namespace std;
int main()
{
	ofstream out("MyMultTable.txt", ios::trunc);
	out<<"Multiplication table:: Made by Tanusree Ghosh"<<endl;
	out<<" 	*1	*2	*3	*4	*5"<<endl;
	for(int i = 1; i<=1000; i++)
	{
		out<<i<<"	";
		for(int j = 1; j<=5; j++)
		{
			out<<i * j<<"	";
		}
		out<<endl;
	}
}
